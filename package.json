{
  "name": "linter-js-standard-engine",
  "version": "1.0.1",
  "description": "Linter plugin for Standard Engine based linters.",
  "main": "init.js",
  "repository": "https://github.com/gustavnikolaj/linter-js-standard-engine.git",
  "author": "Gustav Nikolaj <gustavnikolaj@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gustavnikolaj/linter-js-standard-engine/issues"
  },
  "engines": {
    "atom": ">=1.11.0"
  },
  "files": [
    "init.js",
    "lib"
  ],
  "scripts": {
    "lint": "standard",
    "test": "mocha",
    "coverage": "nyc npm test"
  },
  "homepage": "https://github.com/gustavnikolaj/linter-js-standard-engine#readme",
  "devDependencies": {
    "coveralls": "^2.11.14",
    "debug": "2.2.0",
    "mocha": "^3.1.2",
    "nyc": "^8.3.2",
    "proxyquire": "^1.7.10",
    "standard": "^8.5.0",
    "unexpected": "^10.18.1",
    "unique-temp-dir": "^1.0.0",
    "when": "^3.7.7"
  },
  "standard": {
    "globals": [
      "atom"
    ],
    "ignore": [
      "test/fixtures/faked/*.js"
    ]
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "dependencies": {
    "lru-cache": "^4.0.1",
    "minimatch": "^3.0.3",
    "read-pkg-up": "^2.0.0"
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "html",
      "lcov",
      "text"
    ]
  }
}
